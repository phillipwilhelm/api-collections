#!/usr/bin/env dothttp

// Search for a location by geographic coordinate.

@name("Search for a location by geographic coordinate.")
GET "https://{{baseUrl}}/locations/search"
? "distance"= "<integer>"
? "facebook_places_id"= "<string>"
? "foursquare_id"= "<string>"
? "lat"= "<number>"
? "lng"= "<number>"
? "foursquare_v2_id"= "<string>"



