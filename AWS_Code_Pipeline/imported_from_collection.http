#!/usr/bin/env dothttp

// Returns information about a specified job and whether that job has been received by//             the job worker.
@name(" Acknowledge Job ")
GET "https://{{baseUrl}}/"
? "Action"= "AcknowledgeJob"
? "DryRun"= "<string>"
? "SnapshotId"= "<string>"
? "jobId"= "<string>"
? "nonce"= "<string>"


// Confirms a job worker has received the specified job.
@name(" Acknowledge Third Party Job ")
GET "https://{{baseUrl}}/"
? "Action"= "AcknowledgeThirdPartyJob"
? "DryRun"= "<string>"
? "VolumeId"= "<string>"
? "clientToken"= "<string>"
? "jobId"= "<string>"
? "nonce"= "<string>"


// Creates a new custom action that can be used in all pipelines associated with the//             AWS account.
@name(" Create Custom Action Type ")
GET "https://{{baseUrl}}/"
? "Action"= "CreateCustomActionType"
? "Attribute"= "<string>"
? "DryRun"= "<string>"
? "SnapshotId"= "<string>"
? "category"= "<string>"
? "configurationProperties"= "<string>"
? "inputArtifactDetails"= "<string>"
? "outputArtifactDetails"= "<string>"
? "provider"= "<string>"
? "settings"= "<string>"
? "version"= "<string>"


// Creates a pipeline.
@name(" Create Pipeline ")
GET "https://{{baseUrl}}/"
? "Action"= "CreatePipeline"
? "DryRun"= "<string>"
? "Filter.N"= "<string>"
? "MaxResults"= "<string>"
? "NextToken"= "<string>"
? "Owner.N"= "<string>"
? "RestorableBy.N"= "<string>"
? "SnapshotId.N"= "<string>"
? "pipeline"= "<string>"


// Marks a custom action as deleted.
@name(" Delete Custom Action Type ")
GET "https://{{baseUrl}}/"
? "Action"= "DeleteCustomActionType"
? "Attribute"= "<string>"
? "DryRun"= "<string>"
? "VolumeId"= "<string>"
? "category"= "<string>"
? "provider"= "<string>"
? "version"= "<string>"


// Deletes the specified pipeline.
@name(" Delete Pipeline ")
GET "https://{{baseUrl}}/"
? "Action"= "DeletePipeline"
? "DryRun"= "<string>"
? "Filter.N"= "<string>"
? "MaxResults"= "<string>"
? "NextToken"= "<string>"
? "VolumeId.N"= "<string>"
? "name"= "<string>"


// Prevents artifacts in a pipeline from transitioning to the next stage in the//             pipeline.
@name(" Disable Stage Transition ")
GET "https://{{baseUrl}}/"
? "Action"= "DisableStageTransition"
? "DryRun"= "<string>"
? "Filter.N"= "<string>"
? "MaxResults"= "<string>"
? "NextToken"= "<string>"
? "VolumeId.N"= "<string>"
? "pipelineName"= "<string>"
? "reason"= "<string>"
? "stageName"= "<string>"
? "transitionType"= "<string>"


// Enables artifacts in a pipeline to transition to a stage in a pipeline.
@name(" Enable Stage Transition ")
GET "https://{{baseUrl}}/"
? "Action"= "EnableStageTransition"
? "Device"= "<string>"
? "DryRun"= "<string>"
? "Force"= "<string>"
? "InstanceId"= "<string>"
? "VolumeId"= "<string>"
? "pipelineName"= "<string>"
? "stageName"= "<string>"
? "transitionType"= "<string>"


// Returns information about a job.
@name(" Get Job Details ")
GET "https://{{baseUrl}}/"
? "Action"= "GetJobDetails"
? "DryRun"= "<string>"
? "VolumeId"= "<string>"
? "jobId"= "<string>"


// Returns the metadata, structure, stages, and actions of a pipeline.
@name(" Get Pipeline ")
GET "https://{{baseUrl}}/"
? "Action"= "GetPipeline"
? "Attribute"= "<string>"
? "CreateVolumePermission"= "<string>"
? "DryRun"= "<string>"
? "OperationType"= "<string>"
? "SnapshotId"= "<string>"
? "UserGroup.N"= "<string>"
? "UserId.N"= "<string>"
? "name"= "<string>"
? "version"= "<string>"


// Returns information about an execution of a pipeline, including details about//             artifacts, the pipeline execution ID, and the name, version, and status of the//             pipeline.
@name(" Get Pipeline Execution ")
GET "https://{{baseUrl}}/"
? "Action"= "GetPipelineExecution"
? "AutoEnableIO"= "<string>"
? "DryRun"= "<string>"
? "VolumeId"= "<string>"
? "pipelineExecutionId"= "<string>"
? "pipelineName"= "<string>"


// Returns information about the state of a pipeline, including the stages and//             actions.
@name(" Get Pipeline State ")
GET "https://{{baseUrl}}/"
? "Action"= "GetPipelineState"
? "Attribute"= "<string>"
? "DryRun"= "<string>"
? "SnapshotId"= "<string>"
? "name"= "<string>"


// Requests the details of a job for a third party action.
@name(" Get Third Party Job Details ")
GET "https://{{baseUrl}}/"
? "Action"= "GetThirdPartyJobDetails"
? "Domain"= "<string>"
? "DryRun"= "<string>"
? "clientToken"= "<string>"
? "jobId"= "<string>"


// Gets a summary of all AWS CodePipeline action types associated with your//             account.
@name(" List Action Types ")
GET "https://{{baseUrl}}/"
? "Action"= "ListActionTypes"
? "AllocationId"= "<string>"
? "AllowReassociation"= "<string>"
? "DryRun"= "<string>"
? "InstanceId"= "<string>"
? "NetworkInterfaceId"= "<string>"
? "PrivateIpAddress"= "<string>"
? "PublicIp"= "<string>"
? "actionOwnerFilter"= "<string>"
? "NextToken"= "<string>"


// Gets a summary of all of the pipelines associated with your account.
@name(" List Pipelines ")
GET "https://{{baseUrl}}/"
? "Action"= "ListPipelines"
? "AllocationId.N"= "<string>"
? "DryRun"= "<string>"
? "Filter.N"= "<string>"
? "PublicIp.N"= "<string>"
? "NextToken"= "<string>"


// Returns information about any jobs for AWS CodePipeline to act upon.
@name(" Poll For Jobs ")
GET "https://{{baseUrl}}/"
? "Action"= "PollForJobs"
? "DryRun"= "<string>"
? "Filter.N"= "<string>"
? "MaxResults"= "<string>"
? "NextToken"= "<string>"
? "PublicIp.N"= "<string>"
? "actionTypeId"= "<string>"
? "maxBatchSize"= "<string>"
? "queryParam"= "<string>"


// Determines whether there are any third party jobs for a job worker to act on.
@name(" Poll For Third Party Jobs ")
GET "https://{{baseUrl}}/"
? "Action"= "PollForThirdPartyJobs"
? "AssociationId"= "<string>"
? "DryRun"= "<string>"
? "PublicIp"= "<string>"
? "actionTypeId"= "<string>"
? "maxBatchSize"= "<string>"


// Provides information to AWS CodePipeline about new revisions to a source.
@name(" Put Action Revision ")
GET "https://{{baseUrl}}/"
? "Action"= "PutActionRevision"
? "DryRun"= "<string>"
? "PublicIp"= "<string>"
? "actionName"= "<string>"
? "actionRevision"= "<string>"
? "pipelineName"= "<string>"
? "stageName"= "<string>"


// Provides the response to a manual approval request to AWS CodePipeline.
@name(" Put Approval Result ")
GET "https://{{baseUrl}}/"
? "Action"= "PutApprovalResult"
? "AllocationId"= "<string>"
? "DryRun"= "<string>"
? "PublicIp"= "<string>"
? "actionName"= "<string>"
? "pipelineName"= "<string>"
? "result"= "<string>"
? "stageName"= "<string>"
? "token"= "<string>"


// Represents the failure of a job as returned to the pipeline by a job worker.
@name(" Put Job Failure Result ")
GET "https://{{baseUrl}}/"
? "Action"= "PutJobFailureResult"
? "DryRun"= "<string>"
? "PublicIp"= "<string>"
? "failureDetails"= "<string>"
? "jobId"= "<string>"


// Represents the success of a job as returned to the pipeline by a job worker.
@name(" Put Job Success Result ")
GET "https://{{baseUrl}}/"
? "Action"= "PutJobSuccessResult"
? "Ipv6AddressCount"= "<string>"
? "Ipv6Addresses.N"= "<string>"
? "NetworkInterfaceId"= "<string>"
? "continuationToken"= "<string>"
? "currentRevision"= "<string>"
? "executionDetails"= "<string>"
? "jobId"= "<string>"


// Represents the failure of a third party job as returned to the pipeline by a job//             worker.
@name(" Put Third Party Job Failure Result ")
GET "https://{{baseUrl}}/"
? "Action"= "PutThirdPartyJobFailureResult"
? "AllowReassignment"= "<string>"
? "NetworkInterfaceId"= "<string>"
? "PrivateIpAddress.N"= "<string>"
? "SecondaryPrivateIpAddressCount"= "<string>"
? "clientToken"= "<string>"
? "failureDetails"= "<string>"
? "jobId"= "<string>"


// Represents the success of a third party job as returned to the pipeline by a job//             worker.
@name(" Put Third Party Job Success Result ")
GET "https://{{baseUrl}}/"
? "Action"= "PutThirdPartyJobSuccessResult"
? "DeviceIndex"= "<string>"
? "DryRun"= "<string>"
? "InstanceId"= "<string>"
? "NetworkInterfaceId"= "<string>"
? "clientToken"= "<string>"
? "continuationToken"= "<string>"
? "currentRevision"= "<string>"
? "executionDetails"= "<string>"
? "jobId"= "<string>"


// Resumes the pipeline execution by retrying the last failed actions in a//             stage.
@name(" Retry Stage Execution ")
GET "https://{{baseUrl}}/"
? "Action"= "RetryStageExecution"
? "Description"= "<string>"
? "DryRun"= "<string>"
? "Ipv6AddressCount"= "<string>"
? "Ipv6Addresses.N"= "<string>"
? "PrivateIpAddress"= "<string>"
? "PrivateIpAddresses.N"= "<string>"
? "SecondaryPrivateIpAddressCount"= "<string>"
? "SecurityGroupId.N"= "<string>"
? "SubnetId"= "<string>"
? "pipelineExecutionId"= "<string>"
? "pipelineName"= "<string>"
? "retryMode"= "<string>"
? "stageName"= "<string>"


// Starts the specified pipeline.
@name(" Start Pipeline Execution ")
GET "https://{{baseUrl}}/"
? "Action"= "StartPipelineExecution"
? "DryRun"= "<string>"
? "NetworkInterfaceId"= "<string>"
? "name"= "<string>"


// Updates a specified pipeline with edits or changes to its structure.
@name(" Update Pipeline ")
GET "https://{{baseUrl}}/"
? "Action"= "UpdatePipeline"
? "Attribute"= "<string>"
? "DryRun"= "<string>"
? "NetworkInterfaceId"= "<string>"
? "pipeline"= "<string>"



