// Delete realm

@name("Delete realm [SAT] ")
DELETE "https://{{keycloak_url}}/admin/realms/{{realm}}"
"Content-Type": "application/json"


// Clear cache of external public keys (Public keys of clients or Identity providers)

@name("Clear keys cache [SAT] ")
POST "https://{{keycloak_url}}/admin/realms/{{realm}}/clear-keys-cache"
"Content-Type": "application/json"


// Clear realm cache

@name("Clear realm cache [SAT] ")
POST "https://{{keycloak_url}}/admin/realms/{{realm}}/clear-realm-cache"
"Content-Type": "application/json"


// Clear user cache

@name("Clear user cache [SAT] ")
POST "https://{{keycloak_url}}/admin/realms/{{realm}}/clear-user-cache"
"Content-Type": "application/json"


// Base path for importing clients under this realm.

@name("Base path for importing clients under this realm[SAT] ")
POST "https://{{keycloak_url}}/admin/realms/{{realm}}/client-description-converter"
"Content-Type": "application/json"
data('{ 
	"description": "some description"
}
')


// The key is the client id, the value is the number of sessions that currently are active with that client. Only clients that actually have a session associated with them will be in this map.
// 
// 

@name("Get client session stats Returns a JSON map [SAT]")
GET "https://{{keycloak_url}}/admin/realms/{{realm}}/client-session-stats"
"Content-Type": "application/json"
data('{ 
	"description": "some description"
}
')


// Get credential registrators 

@name("Get credential registrators [SAT]")
GET "https://{{keycloak_url}}/admin/realms/{{realm}}/credential-registrators"
"Content-Type": "application/json"
data('{ 
	"description": "some description"
}
')


// Only name and ids are returned.
// 
// 

@name("Get realm default client scopes [SAT]")
GET "https://{{keycloak_url}}/admin/realms/{{realm}}/default-default-client-scopes"
"Content-Type": "application/json"
data('{ 
	"description": "some description"
}
')


// Update a realm default client scope

@name("Update a realm default client scope [SAT]")
PUT "https://{{keycloak_url}}/admin/realms/{{realm}}/default-default-client-scopes/{{clientScopeId}}"
"Content-Type": "application/json"


// Delete a realm default client scope
// 

@name("Delete a realm default client scope[SAT]")
DELETE "https://{{keycloak_url}}/admin/realms/{{realm}}/default-default-client-scopes/{{clientScopeId}}"
"Content-Type": "application/json"


// Get group hierarchy.
// 
// 

