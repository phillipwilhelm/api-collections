#!/usr/bin/env dothttp

// Get realm info

@name("Get realm info [SAT] ")
GET "https://{{keycloak_url}}/admin/realms/{{realm}}"
"Content-Type": "application/json"


// Get admin events Returns all admin events, or filters events based on URL query parameters listed here

@name("Get admin events of the realm [SAT] ")
GET "https://{{keycloak_url}}/admin/realms/{{realm}}/admin-events"
"Content-Type": "application/json"


// Delete all admin events

@name("Delete all admin events [SAT] ")
DELETE "https://{{keycloak_url}}/admin/realms/{{realm}}/admin-events"
"Content-Type": "application/json"


