#!/usr/bin/env dothttp

// Get projects  merge requests noteable  notes

@name("Get Projects  Merge Requests Noteable  Notes")
GET "https://{{baseUrl}}/projects/:id/merge_requests/:noteable_id/notes"
? "page"= "<>"
? "per_page"= "<>"


// Delete projects  merge requests noteable  notes note

@name("Delete Projects  Merge Requests Noteable  Notes Note")
DELETE "https://{{baseUrl}}/projects/:id/merge_requests/:noteable_id/notes/:note_id"



