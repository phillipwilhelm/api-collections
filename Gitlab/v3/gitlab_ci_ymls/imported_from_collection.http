#!/usr/bin/env dothttp

// This feature was introduced in GitLab 8.9. This endpoint is deprecated and will be removed in GitLab 9.0.

@name("Get the list of the available template")
GET "https://{{baseUrl}}/v3/gitlab_ci_ymls"


// This feature was introduced in GitLab 8.9. This endpoint is deprecated and will be removed in GitLab 9.0.

@name("Get the text for a specific template present in local filesystem")
GET "https://{{baseUrl}}/v3/gitlab_ci_ymls/:name"



